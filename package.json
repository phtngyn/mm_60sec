{
  "name": "nuxt",
  "type": "module",
  "private": true,
  "packageManager": "pnpm@10.28.2",
  "engines": {
    "node": ">=22",
    "pnpm": ">=10"
  },
  "scripts": {
    "build": "nuxt build",
    "dev": "nuxt dev",
    "generate": "nuxt generate",
    "preview": "nuxt preview",
    "postinstall": "nuxt prepare",
    "lint": "npx eslint .",
    "typecheck": "nuxt typecheck"
  },
  "dependencies": {
    "@nuxt/content": "catalog:",
    "better-sqlite3": "catalog:",
    "zod": "catalog:"
  },
  "devDependencies": {
    "@antfu/eslint-config": "catalog:",
    "@nuxt/eslint": "catalog:",
    "@nuxt/ui": "catalog:",
    "@vueuse/core": "catalog:",
    "@vueuse/nuxt": "catalog:",
    "eslint": "catalog:",
    "eslint-plugin-format": "catalog:",
    "nano-staged": "catalog:",
    "nuxt": "catalog:",
    "nuxt-studio": "catalog:",
    "simple-git-hooks": "catalog:",
    "typescript": "catalog:",
    "vue": "catalog:",
    "vue-router": "catalog:",
    "vue-tsc": "catalog:"
  },
  "simple-git-hooks": {
    "pre-commit": "npx nano-staged"
  },
  "nano-staged": {
    "*": "npx eslint . --fix"
  }
